<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="action_student_pdf" model="ir.actions.report">
        <field name="name">Student Report</field>
        <field name="model">school.student</field>
        <field name="report_type">qweb-pdf</field>

        <field name="report_name">school_management_system.report_student_pdf</field>
        <field name="report_file">school_management_system.report_student_pdf</field>

        <field name="print_report_name">
            'Student Report - %s' % (object.display_name)
        </field>
    </record>

</odoo>
